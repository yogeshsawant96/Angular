class Arithmetic 
{
	No1:number;
	No2:number;

	constructor(no1:number,no2:number)
	{
	 this.No1=no1;
	 this.No2=no2;
	}

	Addition():number
	{
	 return this.No1+this.No2;
	}
	Substraction():number
	{
	 return this.No1-this.No2;
	}
	Multiplication():number
	{
	 return this.No1*this.No2;
	}
	Division():number
	{
	return this.No1/this.No2;
	}
}
//Obj1
var obj1=new Arithmetic(10,25);
var iRet=obj1.Addition();
console.log("Addition is "+iRet);
iRet=obj1.Substraction();
console.log("Substraction is "+iRet);
iRet=obj1.Multiplication();
console.log("Multiplication is "+iRet);
iRet=obj1.Division();
console.log("Division is "+iRet);
console.log("\n");
//Obj2
var obj2=new Arithmetic(30,5);
var iRet=obj2.Addition();
console.log("Addition is "+iRet);
iRet=obj2.Substraction();
console.log("Substraction is "+iRet);
iRet=obj2.Multiplication();
console.log("Multiplication is "+iRet);
iRet=obj2.Division();
console.log("Division is "+iRet);
